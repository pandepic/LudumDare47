<?xml version="1.0" encoding="utf-8" ?>

<Templates>
  <Template TemplateName="StandardButton">
    <Label>
      <FontName>LatoBlack</FontName>
      <FontSize>28</FontSize>
      <Position>
        <X>center</X>
        <Y>center</Y>
      </Position>
      <Color>#ffffff</Color>
    </Label>
    <AssetName>StandardButton</AssetName>
    <AssetNamePressed>StandardButton</AssetNamePressed>
    <AssetNameHover>StandardButton</AssetNameHover>
    <AssetNameDisabled>StandardButton</AssetNameDisabled>
    <PreMultiplyAlpha>True</PreMultiplyAlpha>
    <ClickSound>SFXButtonClick</ClickSound>
  </Template>

  <Template TemplateName="MenuButton">
    <Label>
      <FontName>LatoBlack</FontName>
      <FontSize>28</FontSize>
      <Position>
        <X>center</X>
        <Y>center</Y>
      </Position>
      <Color>#ffffff</Color>
    </Label>
    <AssetName>MenuButton</AssetName>
    <AssetNamePressed>MenuButton</AssetNamePressed>
    <AssetNameHover>MenuButton</AssetNameHover>
    <AssetNameDisabled>MenuButton</AssetNameDisabled>
    <PreMultiplyAlpha>True</PreMultiplyAlpha>
    <ClickSound>SFXButtonClick</ClickSound>
  </Template>

  <Template TemplateName="DebugLabel">
    <FontName>LatoBlack</FontName>
    <FontSize>24</FontSize>
    <Color>#FFFFFF</Color>
  </Template>

  <Template TemplateName="SettingsScrollbar">
    <Label>
      <FontName>LatoBlack</FontName>
      <FontSize>16</FontSize>
      <Position>
        <X>center</X>
        <Y>30</Y>
      </Position>
      <Color>#ffffff</Color>
    </Label>
    <Background>
      <Width>500</Width>
      <Left>SettingsScrollbarLeft</Left>
      <Right>SettingsScrollbarRight</Right>
      <Center>SettingsScrollbarCenter</Center>
    </Background>
    <AssetNameSlider>SettingsScrollSlider</AssetNameSlider>
    <AssetNameSliderHover>SettingsScrollSlider</AssetNameSliderHover>
    <SliderOffsetX>0</SliderOffsetX>
  </Template>
</Templates>